<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_v_recipeslistview_ingredientsInPantry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingredients in pantry: "
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/tv_v_recipeslistview_recipeMealTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meal Time"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="18dp" />

    <TextView
        android:id="@+id/tv_v_recipeslistview_recipeName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recipe Name"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="18dp" />

    <TextView
        android:id="@+id/tv_v_recipeslistview_numIngredientsInPantry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="67dp" />
</androidx.constraintlayout.widget.ConstraintLayout>